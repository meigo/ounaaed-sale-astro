---
import { getEntry } from "astro:content";
import PageView from "@lib/components/PageView.astro";

const content = await getEntry("pages", "tehnika");
const imageFiles = Object.values(import.meta.glob("../../assets/images/tehnika/*", { eager: true }));
---

<PageView content={content} images={imageFiles} imagesPath="/tehnika/images/" />
