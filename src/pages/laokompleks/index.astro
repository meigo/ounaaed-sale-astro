---
import { getEntry } from "astro:content";
import PageView from "@lib/components/PageView.astro";

const content = await getEntry("pages", "laokompleks");
const imageFiles = Object.values(import.meta.glob("../../assets/images/laokompleks/*", { eager: true }));
---

<PageView content={content} images={imageFiles} imagesPath="/laokompleks/images/" />
