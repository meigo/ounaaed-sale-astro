---
import MainSectionView from "@lib/components/MainSectionView.astro";
import { getEntry } from "astro:content";

const content = await getEntry("sections", "tehnika");
import image from "@assets/images/main/tehnika.jpg";
import mask from "@assets/svg/mask-circle.svg";
---

<MainSectionView
  color="light"
  imageBgColor="secondary"
  buttonBgColor="secondary"
  buttonTextColor="light"
  {content}
  {image}
  {mask}
  align="left"
/>
