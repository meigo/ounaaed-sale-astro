---
import MainSectionView from "@lib/components/MainSectionView.astro";
import { getEntry } from "astro:content";

const content = await getEntry("sections", "villa");
import image from "@assets/images/main/villa.jpg";
import mask from "@assets/svg/mask-circle.svg";
---

<MainSectionView color="main" imageBgColor="accent" buttonTextColor="main" {content} {image} {mask} />
